<!--pages/Deposit/myProducts/withdrawalConfirmed/index.wxml-->

<view class="p-20 tag  m-20 border-r border-r box-s">
  <view class="p-20 flex jc-sb txt-30 border-b">
    <view class="flex1">认购账户：</view>
    <view class="fw-b flex2">{{list.accountNoName}}</view>
  </view>
  <view class="p-20 flex jc-sb">
    <view class="flex1">币种：</view>
    <view class="fw-b flex2">{{list.currency}}</view>
  </view>
  <view class="p-20 flex jc-sb  ">
    <view class="flex1">产品期限：</view>
    <view class="fw-b flex2">{{list.productTerm}}</view>
  </view>
  <view class="p-20 flex jc-sb ">
    <view class="flex1">下一付息日：</view>
    <view class="fw-b flex2">{{list.nextinterestDate}}</view>
  </view>
  <view class="p-20 flex jc-sb ">
    <view class="flex1">当前收益（元）：</view>
    <view class="fw-b flex2">{{list.totalInsterest}}</view>
  </view>
  <view class="p-20 flex jc-sb">
    <view class="flex1">已派发收益（元）：</view>
    <view class="fw-b flex2 flex">{{list.distributedInterest}}
    </view>
  </view>
</view>
<view class="m-20  btn">
  <van-button class="btn" block color="orange" bindtap="profitQuery">确认支取</van-button>
</view>